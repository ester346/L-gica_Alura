<script>
	
	function pulaLinha() {

		document.write("<br>");	
		document.write("<br>");	
	}

	function mostra(frase) {

		document.write(frase);
		pulaLinha();
	}
    function calculaImc(peso, altura) {
        var imc = Math.round(peso / (altura * altura));
        mostra(" O IMC calculado Ã©: " + imc);
    }
    
    calculaImc(75, 1.77);
    calculaImc(80, 1.80);
    
</script>
